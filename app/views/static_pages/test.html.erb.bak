<!-- app/views/static_pages/test.html.erb -->
<!DOCTYPE html>
<html>
<head>
  <title>Test Page</title>
</head>
<body>
  <h1>Test Page</h1>

<form method="post" action="javascript:alert('SubMitted')" enctype="multipart/form-data"><div class="form">
  <div class="form-body">

    <!-- 会社名 -->
    <div class="form-row">
      <label for="company">会社名</label>
      <div class="form-field">
<input type="text" name="company" id="company" size="60" value="" >
</div>
    </div>
    <!-- /会社名 -->

    <!-- お名前 -->
    <div class="form-row">
      <label for="namae" class="required">お名前</label>
      <div class="form-field form-seimei">
<input type="text" name="sei" id="sei" size="60" value="" >

<input type="text" name="mei" id="mei" size="60" value="" >
</div>
    </div>
    <!-- /お名前 -->

    <!-- ふりがな -->
    <div class="form-row">
      <label for="namae" class="required">ふりがな</label>
      <div class="form-field form-seimei">
<input type="text" name="kana_sei" id="kana_sei" size="60" value="" >

<input type="text" name="kana_mei" id="kana_mei" size="60" value="" >
</div>
    </div>
    <!-- /ふりがな -->

    <!-- 電話番号 -->
    <div class="form-row">
      <label for="tel" class="required">電話番号</label>
      <div class="form-field">
<span class="mwform-tel-field">
	<input type="text" name="tel[data][0]" size="6" maxlength="5" value="" data-conv-half-alphanumeric="true">
	-	<input type="text" name="tel[data][1]" size="5" maxlength="4" value="" data-conv-half-alphanumeric="true">
	-	<input type="text" name="tel[data][2]" size="5" maxlength="4" value="" data-conv-half-alphanumeric="true">
</span>

<input type="hidden" name="tel[separator]" value="-">
</div>
    </div>
    <!-- /電話番号 -->

    <!-- メールアドレス -->
    <div class="form-row">
      <label for="email" class="required">メールアドレス</label>
      <div class="form-field">
<input type="email" name="email" id="email" size="60" value="" placeholder="xxx@sample.com" data-conv-half-alphanumeric="true">
</div>
    </div>
    <!-- /メールアドレス -->

    <!-- 郵便番号 -->
    <div class="form-row">
      <label for="zip">郵便番号</label>
      <div class="form-field">
<span class="mwform-zip-field">
	〒
	<input type="text" name="zip[data][0]" class="zip" size="4" maxlength="3" value="" data-conv-half-alphanumeric="true">
	-	<input type="text" name="zip[data][1]" class="zip" size="5" maxlength="4" value="" data-conv-half-alphanumeric="true">
</span>

<input type="hidden" name="zip[separator]" value="-">
</div>
    </div>
    <!-- /郵便番号 -->

    <!-- 住所 -->
    <div class="form-row">
      <label for="address">住所</label>
      <div class="form-field">
<input type="text" name="address" id="address" size="60" value="" >
</div>
    </div>
    <!-- /住所 -->

    <!-- お問合せ内容 -->
    <div class="form-row">
      <label for="detail">お問合せ内容</label>
      <div class="form-field">
<textarea name="detail" id="detail" cols="50" rows="5"></textarea>
</div>
    </div>
    <!-- /お問合せ内容 -->

  </div>

  <div class="form-hosoku mw_wp_form_confirm-hidden">
    <p class="noorg"><a href="/privacy" target="_blank" rel="noopener">プライバシーポリシー</a></p>
  </div>

  <div class="form-btns">
    
<button type="submit" name="submitConfirm" value="confirm" class="btn btn-arrow">確認画面へ</button>

    
    
  </div>
</div>




</body>
</html>
